<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Bootstrap -->
    <link href="../css/bootstrap.min.css" rel="stylesheet">

    <!-- CSS Files -->
    <link href="../css/bootstrap.min.css" rel="stylesheet" media="screen">
    <link href="../css/font-awesome.min.css" rel="stylesheet">
    <link href="../fonts/icon-7-stroke/css/pe-icon-7-stroke.css" rel="stylesheet">
    <link href="../css/animate.css" rel="stylesheet" media="screen">
    <link href="../css/owl.theme.css" rel="stylesheet">
    <link href="../css/owl.carousel.css" rel="stylesheet">
    <link href="../css/helpdesk.css" rel="stylesheet">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>

    <![endif]-->

    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="../javascripts/bootstrap.min.js"></script>
    <script src="../javascripts/notify.js"></script>
    <!-- Colors -->
    <link href="../css/css-app.css" rel="stylesheet" media="screen">
    <!-- <link href="css/css-app-green.css" rel="stylesheet" media="screen"> -->
    <!-- <link href="css/css-app-purple.css" rel="stylesheet" media="screen"> -->
    <!-- <link href="css/css-app-red.css" rel="stylesheet" media="screen"> -->
    <!-- <link href="css/css-app-orange.css" rel="stylesheet" media="screen"> -->
    <!-- <link href="css/css-app-yellow.css" rel="stylesheet" media="screen"> -->

    <script src="../javascripts/jquery.noty.packaged.js"></script>
    <script src="../javascripts/helpdesk.js"></script>
    <script src="../javascripts/login.js"></script>

    <script src="../javascripts/angular.min.js"></script>
    <script src="../javascripts/angular-resource.min.js"></script>
    <script src="../javascripts/angular-route.min.js"></script>
    <script src="../javascripts/angular-cookies.min.js"></script>
    <script src="../javascripts/angular-ngStorage.js"></script>

    <script src="../javascripts/controller/angular-controller.js"></script>
    <script src="../javascripts/service/angular-services.js"></script>


    <!-- Google Fonts -->
    <link rel="stylesheet"
          href="http://fonts.googleapis.com/css?family=Lato:100,300,400,700,900,100italic,300italic,400italic,700italic,900italic"/>
</head>
<body ng-app="helpDeskApp">


<div  ng-controller="profileController" id="wrapper" class="container-fluid">
    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <div >
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <span class="navbar-brand" >{{profile.roles[0].name}}</span>
            </div>
            <!-- Top Menu Items -->
            <ul class="nav navbar-right top-nav">
                <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-user"></i>{{profile.firstname}} {{profile.lastname}}<b
                                class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li>
                            <a href="#" ng-click="viewProfile();"><i class="fa fa-fw fa-user"></i> Profile</a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="#" ng-click="logout()"><i class="fa fa-fw fa-power-off"></i> Log Out</a>
                        </li>
                    </ul>
                </li>
            </ul>
        </div>
        <!-- /.navbar-collapse -->
    </nav>

    <div class="col-md-offset-6 col-md-6" style="margin-top: 80px">
        <div class="head-btn wow fadeInLeft">
            <a ng-if="profile.roles[0].name=='Customer'" href="/ticket"
               class="btn-primary">Add New Ticket</a>
            <a ng-if="profile.roles[0].name=='Admin'" href="/ticket/adminTickets"
               class="btn-primary">Show Tickets</a>
            <a ng-if="profile.roles[0].name=='Customer'" href="/ticket/myTickets"
               class="btn-primary">Show Tickets</a>
        </div>
    </div>


    <div class="container" style="margin-top: 100px">
        <div class="row">
            <div class="col-md-4 intro-pic wow slideInLeft">
                <img src="../images/home-head.jpg" alt="image" class="img-responsive">
            </div>
            <div class="col-md-8 container">
                <div class="col-sm-offset-3 col-sm-7">
                    <label for="userName">User Name</label>
                    <input type="text" ng-model="userEntry.username" class="form-control input-sm"
                           name="username"
                           id="username" disabled
                           placeholder="User Name" required>
                    <input type="hidden" ng-model="userEntry._id" ng-value="userEntry._id">
                </div>
                <div id="update-profile" class="form-group">
                    <div class="col-sm-offset-3 col-sm-7">
                        <label for="firstname">First name</label>
                        <input type="text" ng-model="userEntry.firstname" class="form-control input-sm"
                               name="firstname"
                               id="firstname"
                               placeholder="First Name" required>
                    </div>
                    <div class="col-sm-offset-3 col-sm-7">
                        <label for="lastname">Last name</label>
                        <input type="text" ng-model="userEntry.lastname" class="form-control input-sm"
                               name="lastname"
                               id="lastname"
                               placeholder="Last Name" required>
                    </div>
                    <div class="col-sm-offset-3 col-sm-7">
                        <label for="email">Email ID</label>
                        <input type="text" ng-model="userEntry.email" class="form-control input-sm" name="email"
                               id="email"
                               placeholder="Email ID" required>
                    </div>
                    <div class="col-sm-offset-3 col-sm-7">
                        <label for="mobile">Mobile Number</label>
                        <input type="text" ng-model="userEntry.mobile" class="form-control input-sm" name="mobile"
                               id="mobile"
                               placeholder="Mobile Number" required>
                    </div>


                    <div class=" col-md-offset-2 col-md-9">
                        <div class="col-sm-offset-3 col-sm-7">
                            <input type="submit" ng-click="update(userEntry)" class="btn-default"
                                   value="Update"/>

                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>


<footer id="footer">
    <div class="container">
        <div class="col-sm-4 col-sm-offset-4">
            <!-- /.social links -->
            <div class="social text-center">
                <ul>
                    <li><a class="wow fadeInUp" href="https://twitter.com/"><i class="fa fa-twitter"></i></a></li>
                    <li><a class="wow fadeInUp" href="https://www.facebook.com/" data-wow-delay="0.2s"><i
                                    class="fa fa-facebook"></i></a></li>
                    <li><a class="wow fadeInUp" href="https://plus.google.com/" data-wow-delay="0.4s"><i
                                    class="fa fa-google-plus"></i></a></li>
                    <li><a class="wow fadeInUp" href="https://instagram.com/" data-wow-delay="0.6s"><i
                                    class="fa fa-instagram"></i></a></li>
                </ul>
            </div>
            <div class="text-center wow fadeInUp" style="font-size: 14px;">Copyright <a
                        href="http://www.mobiquityinc.com">Mobiquity Softtech Pvt Ltd.</a></div>
            <a href="#" class="scrollToTop"><i class="pe-7s-up-arrow pe-va"></i></a>
        </div>
    </div>
</footer>
</body>
</html>